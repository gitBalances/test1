<script type="text/javascript" th:inline="none">

    /*获取json数据中的某一个字段*/
    function getParamValues(name,json){
        var ret = [];
        var len = Object.keys(json).length;

        for (var i = 0; i < len; i++) {
            ret.push(json[i][name]);
        }
        return ret;
    }
    /*获取json数组长度*/
